{
  "name": "blockchain_manager",
  "version": "1.0.0",
  "description": "Blockchain Manager API",
  "main": "index.js",
  "scripts": {
    "start": "ts-node index.ts",
    "chainUp": "python3 scripts/build_fabric.py -O 2 -p 1 -u 1 -o 0 -d testcompany -n testchain -C TestChannel -c 1,2 --block 1s 10 2MB --folder bc_test",
    "mongoUp": "bash scripts/mongo.sh up",
    "mongoDown": "bash scripts/mongo.sh down",
    "caliperUp": "python3 scripts/build_caliper.py -O 2 -p 1 -C TestChannel 1 -c 1,2 -o 1 -d testcompany --contracts test_contract,1.0",
    "contractUp": "python3 scripts/uploadCC.py -O 2 -p 1 -C TestChannel -c 1,2 -d testcompany -n test_contract1 -v 1.0 --folder bc_test",
    "clean": "bash scripts/clean.sh",
    "updateCCP": "node scripts/updateConfig.js",
    "rmcc": "bash scripts/removeCC.sh"
  },
  "author": "Jorge Castillo",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "consolidate": "^1.0.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.10",
    "express": "^4.17.1",
    "express-session": "^1.18.1",
    "fabric-ca-client": "beta",
    "fabric-network": "beta",
    "fs": "0.0.1-security",
    "html": "^1.0.0",
    "mongoose": "^5.10.0",
    "multer": "^1.4.2",
    "object-hash": "^2.0.3",
    "python-shell": "^3.0.1",
    "simple-statistics": "^7.7.0",
    "swig": "^1.4.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.4.3",
    "winston": "^3.17.0"
  }
}
